                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select * 
from (
		select ROW_ID AS ID_OPORTUNIDAD,
			   'PAN' AS PAIS,
			   CREATED AS FECHA_CREACION, 
			   LAST_UPD AS FECHA_MODIFICACION,
			   YEAR(LAST_UPD) AS AÑO_MODIFICACION,
			   MONTH(LAST_UPD) AS MES_MODIFICACION,
			   'Tarjeta Nueva' as PRODUCTO,
				'Chatbot' as CANAL,
				CASE WHEN STATUS_CD IN ('Interesado','Aceptado','Generado','Abierto') THEN 'En proceso' 
					 when status_cd = 'Vencido' then 'Perdido'
					 else STATUS_CD end as ESTADO,
				STATUS_CD AS ETAPA_ACTUAL,
				REASON_WON_LOST_CD AS RAZON,
				CREATED_BY AS USUARIO_APERTURA,
				LAST_UPD_BY AS USUARIO_MODIFICA,
				'SIEBEL' AS ORIGEN,
				ROW_ID AS PAR_ID_OPORTUNIDAD,
				0 AS FLAG_CLEAN
		FROM [DWSiebel].[PAN].[S_OPTY] 
		WHERE OPTY_CD = 'PREAPROBADO'
)a
WHERE MES_MODIFICACION = 10 AND AÑO_MODIFICACION = 2024</property>
